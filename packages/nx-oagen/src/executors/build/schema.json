{
    "version": 2,
    "outputCapture": "direct-nodejs",
    "$schema": "http://json-schema.org/schema",
    "cli": "nx",
    "title": "Build executor",
    "description": "",
    "type": "object",
    "properties": {
        "specFile": {
            "description": "path to swagger spec file",
            "type": "string"
        },
        "outDir": {
            "description": "output directory (defaults to sourceRoot/lib/)",
            "type": "string",
            "default": "lib/"
        },
        "generator": {
            "description": "generator to use (defaults to typescript-axios)",
            "type": "string",
            "default": "typescript-axios"
        },
        "additionalProperties": {
            "type": "object"
        }
    },
    "required": ["specFile"]
}
