{
    "name": "nx-oagen",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/nx-oagen/src",
    "projectType": "library",
    "targets": {
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/nx-oagen/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/packages/nx-oagen"],
            "options": {
                "jestConfig": "packages/nx-oagen/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/packages/nx-oagen",
                "tsConfig": "packages/nx-oagen/tsconfig.lib.json",
                "packageJson": "packages/nx-oagen/package.json",
                "main": "packages/nx-oagen/src/index.ts",
                "assets": [
                    "packages/nx-oagen/*.md",
                    {
                        "input": "./packages/nx-oagen/src",
                        "glob": "**/!(*.ts)",
                        "output": "./src"
                    },
                    {
                        "input": "./packages/nx-oagen/src",
                        "glob": "**/*.d.ts",
                        "output": "./src"
                    },
                    {
                        "input": "./packages/nx-oagen",
                        "glob": "generators.json",
                        "output": "."
                    },
                    {
                        "input": "./packages/nx-oagen",
                        "glob": "executors.json",
                        "output": "."
                    },
                    {
                        "input": "./",
                        "glob": "LICENSE.md",
                        "output": "."
                    }
                ]
            }
        }
    },
    "tags": []
}
